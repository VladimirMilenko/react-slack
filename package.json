{
    "name": "slack-renderer",
    "version": "1.0.0",
    "main": "app.js",
    "license": "MIT",
    "dependencies": {
        "lodash": "4.17.15",
        "react": "^16.12.0",
        "react-reconciler": "^0.24.0",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/preset-react": "^7.8.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "parcel": "^1.12.4",
        "react-devtools": "^4.4.0"
    },
    "scripts": {
        "start:node": "parcel --target node app.js",
        "build": "yarn build:node && yarn build:browser",
        "build:node": "parcel build -d build/node --target node app.js",
        "build:browser": "parcel build -d build/browser --target browser app.js"
    }
}
